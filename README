ejecicio 1
println ("horas trabajo")
val horastrabajo=readfloat()
println ("pago hora")
val pagohora=readfloat()
println ("pago normal")
val pagonormal=readfloat()
val pagonormal=horatrabajo*pagohora
println ("horas extras")
val horasextras=readfloat()
println ("pago hora extra")
val pagohoraextra=readfloat()
println ("pago extra")
val pagoextra=readfloat()
val pagoextra=horasextras*pagohoraextra+pagonormal
val pagototal=pagonormal+pagoextra
println ("pagototal="+pagototal)
